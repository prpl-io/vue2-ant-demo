{
    "name": "demo-vue-2-app",
    "version": "1.0.0",
    "description": "Vue 2 demo",
    "main": "index.js",
    "engines": {
        "node": "13.14.0",
        "npm": "6.14.4"
    },
    "scripts": {
        "start": "node ./src/index.js",
        "server": "node ./src/bin/server.js",
        "eslint": "node ./src/scripts/eslint.js",
        "dev": "nodemon ./src/bin/server.js",
        "queues": "nodemon ./src/bin/queues.js",
        "db-undo-migrate": "./node_modules/.bin/sequelize db:migrate:undo:all",
        "db-migrate": "./node_modules/.bin/sequelize db:migrate",
        "db-seed": "./node_modules/.bin/sequelize db:seed:all",
        "db-drop": "./node_modules/.bin/sequelize db:drop",
        "db-create": "./node_modules/.bin/sequelize db:create",
        "db-setup": "npm run db-undo-migrate && npm run db-migrate && npm run db-seed",
        "db-setup-fresh": "npm run db-drop && npm run db-create && npm run db-migrate && npm run db-seed",
        "redis-cache-flush": "docker-compose exec redis_cache redis-cli -a 'pass' FLUSHALL",
        "redis-session-flush": "docker-compose exec redis_session redis-cli -a 'pass' FLUSHALL",
        "test": "jest --verbose --runInBand --detectOpenHandles",
        "testp": "jest --verbose --runInBand --detectOpenHandles --",
        "test-pipeline": "jest --verbose --runInBand --detectOpenHandles"
    },
    "repository": "none",
    "license": "ISC",
    "dependencies": {
        "@sentry/node": "^5.27.4",
        "@types/faker": "^5.5.6",
        "amqplib": "^0.6.0",
        "atob": "^2.1.2",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "btoa": "^1.2.1",
        "bull": "^3.11.0",
        "connect-redis": "^3.4.2",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "csv-parser": "^3.0.0",
        "csvtojson": "^2.0.10",
        "dayjs": "^1.10.4",
        "deepmerge": "^4.2.2",
        "del": "^6.0.0",
        "dotenv": "^9.0.2",
        "express": "^4.17.1",
        "express-fileupload": "^1.2.1",
        "express-session": "^1.16.2",
        "express-validator": "^6.5.0",
        "faker": "^5.5.3",
        "google-auth-library": "^7.6.2",
        "helmet": "^3.21.1",
        "http-status-codes": "^2.1.4",
        "image-size": "^0.9.1",
        "jest": "^24.9.0",
        "joi": "^17.4.2",
        "lodash": "^4.17.21",
        "mysql2": "^1.7.0",
        "node-cron": "^2.0.3",
        "node-dependency-injection": "^2.3.5",
        "nodemailer": "^6.3.0",
        "nodemailer-express-handlebars": "^4.0.0",
        "nodemon": "^1.19.1",
        "pg": "^7.12.1",
        "pg-hstore": "^2.3.3",
        "proper-lockfile": "^4.1.1",
        "querystring": "^0.2.0",
        "redis-sessions": "^2.1.0",
        "sequelize": "^6.6.2",
        "sequelize-cli": "^6.2.0",
        "sharp": "^0.28.2",
        "supertest": "^4.0.2",
        "swagger-jsdoc": "^6.1.0",
        "urlencode": "^1.1.0",
        "uuid": "^3.3.3",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "eslint": "^7.31.0"
    }
}
